<!--components/w-swiper/w-swiper.wxml-->
<swiper class="swiper" 
circular autoplay interval="2000" 
indicator-dots 
indicator-active-color="#fff">
	<block wx:for="{{list}}" wx:key="index">
		<swiper-item class="swiper-item">
      <image src="{{item.image || item}}" mode="widthFix"/>
    </swiper-item>
	</block>
</swiper>
